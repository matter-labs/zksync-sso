{
  "name": "zksync-sso-web-sdk",
  "version": "0.0.1",
  "description": "WebAssembly-based SDK for zkSync SSO ERC-4337 integration",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./bundler": {
      "import": "./dist/bundler.js",
      "types": "./dist/bundler.d.ts"
    },
    "./node": {
      "import": "./dist/node.js",
      "types": "./dist/node.d.ts"
    }
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:wasm && npm run build:ts",
    "build:wasm": "RUSTFLAGS='--cfg getrandom_backend=\"wasm_js\"' wasm-pack build ../rust/zksync-sso-erc4337/crates/zksync-sso-erc4337-ffi-web --target bundler --out-dir ../../../../web/pkg-bundler && RUSTFLAGS='--cfg getrandom_backend=\"wasm_js\"' wasm-pack build ../rust/zksync-sso-erc4337/crates/zksync-sso-erc4337-ffi-web --target nodejs --out-dir ../../../../web/pkg-node",
    "build:ts": "tsc && rollup -c",
    "clean": "rm -rf dist/ pkg-bundler/ pkg-node/",
    "dev": "tsc --watch",
    "test": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.0.0",
    "eslint": "^8.57.0",
    "rollup": "^4.17.2",
    "rollup-plugin-dts": "^6.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "wasm-pack": "^0.12.1"
  },
  "keywords": [
    "zksync",
    "sso",
    "erc4337",
    "account-abstraction",
    "wasm",
    "webassembly",
    "web3",
    "ethereum"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/matter-labs/zksync-sso.git",
    "directory": "packages/sdk-platforms/web"
  },
  "homepage": "https://github.com/matter-labs/zksync-sso#readme",
  "bugs": {
    "url": "https://github.com/matter-labs/zksync-sso/issues"
  }
}