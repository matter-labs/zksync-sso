{
  "compilerOptions": {
    "rootDir": ".",
    "allowUnreachableCode": true,
    "allowUnusedLabels": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "lib": [
      "ESNext"
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": false,
    "target": "ESNext",
    "verbatimModuleSyntax": true,
    "noCheck": true
  },
  "include": [
    "src/index.tsx",
    "src/NativeZksyncSso.ts",
    "src/passkey/**/*",
    "example/src/**/*"
  ],
  "exclude": [
    "example",
    "lib",
    "src/generated/**/*",
    "node_modules/**/*"
  ]
}