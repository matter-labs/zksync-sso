{
  "hosting": [
    {
      "target": "zksync-auth-server-staging",
      "trailingSlash": false,
      "public": "packages/auth-server/.output/public",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "headers": [
        {
          "source": "/.well-known/apple-app-site-association",
          "headers": [
            {
              "key": "Content-Type",
              "value": "application/json"
            }
          ]
        }
      ]
    }
  ]
}
